# Firebase Configuration
# Replace these with your actual Firebase project credentials
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Financial Data API Keys
# Alpha Vantage - Free tier: 5 requests per minute, 500 per day
# Get your free key at: https://www.alphavantage.co/support/#api-key
REACT_APP_ALPHA_VANTAGE_API_KEY=demo

# IEX Cloud - Free tier: 500,000 requests per month
# Get your free key at: https://iexcloud.io/
REACT_APP_IEX_CLOUD_API_KEY=pk_test

# AI/LLM API Keys (for future AI report generation)
# OpenAI API Key
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude API Key  
REACT_APP_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Environment Settings
NODE_ENV=development
REACT_APP_ENV=development